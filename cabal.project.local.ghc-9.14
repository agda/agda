with-compiler: ghc-9.14

flags:       +enable-cluster-counting +debug

-- allow-newer for non-GHC packages
allow-newer: *:QuickCheck

-- allow-newer for GHC-shipped packages
allow-newer: *:Cabal
allow-newer: *:Cabal-syntax
allow-newer: *:array
allow-newer: *:base
allow-newer: *:binary
allow-newer: *:bytestring
allow-newer: *:containers
allow-newer: *:deepseq
allow-newer: *:directory
allow-newer: *:exceptions
